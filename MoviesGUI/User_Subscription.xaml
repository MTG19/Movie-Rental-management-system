<Window x:Class="MoviesGUI.User_Subscription"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MoviesGUI"
        mc:Ignorable="d"
        Title="User_Subscription" Height="450" Width="800">
    <Grid Background="#FFD9ECFF">
        <TextBlock x:Name="End_Date_Label" TextWrapping="Wrap" RenderTransformOrigin="2.739,-2.504" FontSize="18" Margin="20,224,0,185" HorizontalAlignment="Left"><Run Text="Subscription End Date: "/><LineBreak/><Run/><LineBreak/><Run/></TextBlock>
        <TextBlock x:Name="Title" TextWrapping="Wrap" RenderTransformOrigin="2.739,-2.504" FontSize="28" Margin="0,14,0,379" HorizontalAlignment="Center" FontWeight="Bold"><Run Text="Subscription Info"/><LineBreak/><Run/><LineBreak/><Run/></TextBlock>
        <TextBlock x:Name="Start_Date" TextWrapping="Wrap" RenderTransformOrigin="2.739,-2.504" FontSize="18" Margin="22,154,0,255" HorizontalAlignment="Left" FontWeight="Bold"><Run Text="The start date"/><LineBreak/><Run/></TextBlock>
        <TextBlock x:Name="Start_Date_Label" TextWrapping="Wrap" RenderTransformOrigin="2.739,-2.504" FontSize="18" Margin="19,126,0,283" HorizontalAlignment="Left"><Run Text="Subscription start Date: "/><LineBreak/><Run/></TextBlock>
        <TextBlock x:Name="End_Date" TextWrapping="Wrap" RenderTransformOrigin="2.739,-2.504" FontSize="18" Margin="20,254,0,155" HorizontalAlignment="Left" FontWeight="Bold"><Run Text="The end date"/><LineBreak/><Run/><LineBreak/><Run/></TextBlock>
        <Button x:Name="Extend_subs_btn" Content="Extend your subscription" Margin="314,341,314,61" Background="#FF3B82F6" Click="Button_Click_3" FontWeight="Bold"/>
    </Grid>
</Window>

