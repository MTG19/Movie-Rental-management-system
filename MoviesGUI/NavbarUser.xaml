<UserControl x:Class="MoviesGUI.NavbarUser"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MoviesGUI"
             mc:Ignorable="d" 
             Height="70"  Background="white">
    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" >
        <Button Content="Profile" Margin="15" Padding="7" BorderBrush="Blue" Click="Profile_Click" Background="AliceBlue"/>
        <Image Source="Images/logo.png" Width="100" Height="70" Margin="1"/>
        <Button Content="Movies" Margin="15"  Padding="7" BorderBrush="Blue" Click="Movies_Click" Background="AliceBlue"/>
        
    </StackPanel>
</UserControl>
